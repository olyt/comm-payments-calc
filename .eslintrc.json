{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "prettier"
  ],
//  "plugins": [
//    "prettier"
//  ],
  "rules": {
    "no-trailing-spaces": [
      "error",
      {
        "skipBlankLines": true
      }
    ],
    "no-return-assign": 0,
    "eol-last": 0,
    "semi": 0,
    "camelcase": "error",
    "object-curly-spacing": 0,
    "comma-dangle": 0,
//    "prettier/prettier": "error",
    "quotes": [
      "error",
      "single"
    ]
  },
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "17.0.2",
      "flowVersion": "0.86"
    },
    "propWrapperFunctions": [
      "forbidExtraProps"
    ]
  }
}